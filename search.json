---
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "content"  : "{{ post.excerpt | strip_html | strip_newlines | truncatewords: 25 | downcase | remove: '.' | remove: ',' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]